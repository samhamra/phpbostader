<!-- Fungerar inte självständigt - måste kompletteras här i början! -->

[
proc courserefexpand {course_v_name} {
    upvar $course_v_name course_v
    set res ""
    foreach el [array names course_v] {
	append res "[länk [lindex $course_v($el) 1] [lindex $course_v($el) 0]]\n"
	append res "- [lindex $course_v($el) 2]\n[stycke]\n"
    }
    return $res
}

proc own_home_page_refexpand {ref name} {
    if {[string length "$ref"] > 0} {
	return "[länk $ref "min hemsida"] (${name}s)"
    } else {
	return ""
    }
}
]

